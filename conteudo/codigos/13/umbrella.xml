<?xml version="1.0" encoding="UTF-8"?>
<BIF VERSION="0.3"  xmlns="http://www.cs.ubc.ca/labs/lci/fopi/ve/XMLBIFv0_3"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.cs.ubc.ca/labs/lci/fopi/ve/XMLBIFv0_3 http://www.cs.ubc.ca/labs/lci/fopi/ve/XMLBIFv0_3/XMLBIFv0_3.xsd">
<NETWORK>
<NAME>Simple Umbrella Decision</NAME>
<PROPERTY>detailed = </PROPERTY>
<PROPERTY>short = This is a very simple decision problem built by Ross D. Shachter modeling the decision of whether or not to bring an umbrella based on the weather forecast.</PROPERTY>

<VARIABLE TYPE="nature">
	<NAME>Weather</NAME>
	<OUTCOME>sun</OUTCOME>
	<OUTCOME>rain</OUTCOME>
	<PROPERTY>position = (7408.0009765625, 5245.474609375)</PROPERTY>
</VARIABLE>

<VARIABLE TYPE="nature">
	<NAME>Forecast</NAME>
	<OUTCOME>good</OUTCOME>
	<OUTCOME>bad</OUTCOME>
	<PROPERTY>position = (7411.69287109375, 5397.3076171875)</PROPERTY>
</VARIABLE>

<VARIABLE TYPE="decision">
	<NAME>Umbrella</NAME>
	<OUTCOME>Take</OUTCOME>
	<OUTCOME>Leave</OUTCOME>
	<PROPERTY>position = (7406.27197265625, 5075.525390625)</PROPERTY>
</VARIABLE>

<VARIABLE TYPE="utility">
	<NAME>U</NAME>
	<PROPERTY>position = (7610.3076171875, 5184.525390625)</PROPERTY>
</VARIABLE>

<DEFINITION>
	<FOR>Weather</FOR>
	<TABLE>0.7 0.3</TABLE>
</DEFINITION>

<DEFINITION>
	<FOR>Forecast</FOR>
	<GIVEN>Weather</GIVEN>
	<TABLE>0.8 0.2 0.1 0.9</TABLE>
</DEFINITION>

<DEFINITION>
	<FOR>Umbrella</FOR>
	<TABLE>1</TABLE>
</DEFINITION>

<DEFINITION>
	<FOR>U</FOR>
	<GIVEN>Weather</GIVEN>
	<GIVEN>Umbrella</GIVEN>
	<TABLE>20.0 100.0 70.0 0.0</TABLE>
</DEFINITION>
</NETWORK>
</BIF>
